{
  "name": "@patternplate/plugin-sketch-export",
  "version": "2.5.2",
  "description": "Export patternplate components to Sketch",
  "browser": "./browser.js",
  "scripts": {
    "clean": "rimraf lib",
    "start": "concurrently \"tsc -w --preserveWatchOutput\" \"webpack -w\"",
    "build": "concurrently \"tsc\" \"webpack\"",
    "watch": "yarn start",
    "deps": "#dependency-check . --missing && dependency-check . --extra --no-dev"
  },
  "files": [
    "browser.js",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patternplate/patternplate.git"
  },
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "xo": false,
  "dependencies": {
    "@brainly/html-sketchapp": "^3.1.0",
    "puppeteer": "^1.3.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "dependency-check": "^3.1.0",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
