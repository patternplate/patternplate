{
  "name": "@patternplate/search",
  "version": "3.0.0-alpha.0",
  "description": "Perform search queries on a list of patternplate items",
  "scripts": {
    "start": "yarn build -w",
    "build": "babel -D src -d lib --source-maps --ignore **/*.test.js",
    "clean": "rimraf lib",
    "watch": "yarn build -w",
    "deps": "dependency-check . --missing && dependency-check . --extra --no-dev"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "xo": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patternplate/patternplate.git"
  },
  "bugs": {
    "url": "https://github.com/patternplate/patternplate/issues"
  },
  "homepage": "https://github.com/patternplate/patternplate#readme",
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "fuse.js": "^3.2.0",
    "lodash.flatten": "^4.4.0",
    "lodash.intersection": "^4.4.0",
    "logic-query-parser": "^0.0.5",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@patternplate/babel-preset": "^3.0.0-alpha.0",
    "dependency-check": "^3.1.0",
    "fixturez": "^1.1.0"
  }
}
