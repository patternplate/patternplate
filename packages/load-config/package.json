{
  "name": "@patternplate/load-config",
  "version": "3.0.0-alpha.0",
  "description": "Load patternplate config",
  "scripts": {
    "start": "yarn build -w",
    "build": "babel -D src -d lib --source-maps --ignore **/*.test.js",
    "clean": "rimraf lib",
    "watch": "yarn build -w",
    "deps": "dependency-check . --missing && dependency-check . --extra --no-dev -i @patternplate/render-default"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "xo": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patternplate/patternplate.git"
  },
  "bugs": {
    "url": "https://github.com/patternplate/patternplate/issues"
  },
  "homepage": "https://github.com/patternplate/patternplate#readme",
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@patternplate/render-default": "^2.5.18",
    "cosmiconfig": "^3.1.0",
    "resolve-from": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@patternplate/babel-preset": "^3.0.0-alpha.0",
    "dependency-check": "^3.1.0",
    "fixturez": "^1.1.0"
  }
}
