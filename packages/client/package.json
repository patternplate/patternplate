{
  "name": "@patternplate/client",
  "version": "2.0.0-4",
  "description": "Universal javascript client application for patternplate",
  "main": "lib/server.js",
  "scripts": {
    "build": "concurrently \"npx webpack\" \"babel src -d lib --source-maps\"",
    "clean": "npx rimraf lib",
    "watch": "concurrently \"npx webpack -w\" \"babel src -d lib --source-maps -w\""
  },
  "xo": false,
  "babel": {
    "presets": [
      [
        "env",
        {
          "exclude": [
            "transform-regenerator",
            "transform-async-to-generator"
          ]
        }
      ],
      "react"
    ],
    "plugins": [
      "fast-async",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "sinnerschrader/patternplate-client"
  },
  "keywords": [
    "patternplate"
  ],
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Daniel GooÃŸ",
      "email": "daniel.gooss@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "dependencies": {
    "@marionebl/react-router": "^2.6.2",
    "@patternplate/api": "^2.0.0-4",
