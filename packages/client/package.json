{
  "name": "@patternplate/client",
  "version": "2.0.9",
  "description": "Universal javascript client application for patternplate",
  "main": "./server.js",
  "scripts": {
    "start": "yarn watch",
    "babel": "babel src -d lib --source-maps",
    "build": "concurrently \"webpack --config webpack.prod.js\" \"yarn babel\"",
    "clean": "npx rimraf lib",
    "watch": "concurrently \"webpack -w\" \"yarn babel -w\"",
    "prepublishOnly": "yarn build",
    "deps": "dependency-check . render.js eject.js . --missing && dependency-check . render.js eject.js --extra --no-dev -i @babel/runtime"
  },
  "xo": false,
  "babel": {
    "presets": [
      [
        "module:@patternplate/babel-preset",
        {
          "targets": [
            "node"
          ],
          "sources": [
            "react",
            "styled-components"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "patternplate/patternplate"
  },
  "keywords": [
    "patternplate"
  ],
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Daniel GooÃŸ",
      "email": "daniel.gooss@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "dependencies": {
    "@marionebl/react-router": "2.6.3",
    "@patternplate/api": "^2.0.9",
    "@patternplate/components": "^2.0.9",
    "@patternplate/load-config": "^2.0.9",
    "@patternplate/load-docs": "^2.0.9",
    "@patternplate/load-meta": "^2.0.9",
    "@patternplate/search": "^2.0.9",
    "@patternplate/websocket-client": "^2.0.9",
    "@patternplate/widgets": "^2.0.9",
    "arson": "^0.2.6",
    "btoa": "^1.1.2",
    "express": "^4.16.2",
    "globby": "^8.0.1",
    "isomorphic-fetch": "^2.0.2",
    "lodash": "^4.15.0",
    "memory-fs": "^0.4.1",
    "platform": "^1.3.1",
    "raf": "^3.3.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-redux": "^4.0.5",
    "react-transition-group": "^2.2.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-thunk": "^2.1.0",
    "remark": "^9.0.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "reselect": "^3.0.1",
    "scrollparent": "^1.0.0",
    "semver": "^5.4.1",
    "serve-static": "^1.13.2",
    "tag-hoc": "^1.0.0-0",
    "topologically-combine-reducers": "0.0.5",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@patternplate/babel-preset": "^2.0.9",
    "babel-loader": "8.0.0-beta.0",
    "concurrently": "^3.5.0",
    "dependency-check": "^3.1.0",
    "dom4": "^2.0.1",
    "fast-async": "^6.3.0",
    "front-matter": "^2.1.2",
    "fuse.js": "^2.2.0",
    "hast-to-hyperscript": "^2.0.4",
    "highlight.js": "^9.6.0",
    "lowlight": "^1.2.0",
    "react-addons-text-content": "^0.0.4",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "3",
    "webpack-cli": "1"
  }
}
