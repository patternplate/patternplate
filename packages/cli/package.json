{
  "name": "@patternplate/cli",
  "version": "2.0.0-4",
  "description": "Create, show and deliver component libraries",
  "bin": {
    "patternplate": "./lib/cli.js"
  },
  "scripts": {
    "babel": "babel src --out-dir ./lib",
    "build": "npx yarn run babel",
    "clean": "npx rimraf lib",
    "watch": "npm run babel -- --watch"
  },
  "xo": false,
  "babel": {
    "presets": [
      [
        "env",
        {
          "exclude": [
            "transform-regenerator",
            "transform-async-to-generator"
          ]
        }
      ]
    ],
    "plugins": [
      "fast-async"
    ]
  },
  "dependencies": {
    "@patternplate/client": "^2.0.0-4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-slash": "^2.0.1",
    "meow": "^3.7.0",
    "ora": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "fast-async": "^6.3.0",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "sinnerschrader/patternplate"
  },
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  }
}
