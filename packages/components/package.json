{
  "name": "@patternplate/components",
  "version": "2.0.0-4",
  "description": "ui component library for patternplate-client",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently \"patternplate\" \"npm run watch\"",
    "build": "babel src -d lib --source-maps",
    "watch": "babel src -d lib --source-maps -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sinnerschrader/patternplate.git"
  },
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Daniel GooÃŸ",
      "email": "daniel.gooss@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "xo": false,
  "babel": {
    "sourceMaps": true,
    "presets": [
      "react",
      [
        "env",
        {
          "modules": false,
          "targets": {
            "node": "4"
          }
        }
      ]
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@patternplate/cli": "^2.0.0-4",
    "@patternplate/render-styled-components": "^2.0.0-4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browserify": "^14.4.0",
    "lorem-ipsum": "^1.0.4",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "color": "^2.0.0",
    "concurrently": "^3.5.1",
    "d3-force": "^1.0.6",
    "front-matter": "^2.3.0",
    "hast-to-hyperscript": "^3.0.2",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "lowlight": "^1.9.1",
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-addons-text-content": "^0.0.4",
    "react-clipboard.js": "^1.1.2",
    "react-dom": "16.2.0",
    "react-side-effect": "^1.0.2",
    "react-vis": "^1.7.5",
    "remark": "^8.0.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-react": "^4.0.1",
    "styled-components": "^2.1.2",
    "tag-hoc": "^1.0.0",
    "vis": "^4.20.1"
  }
}
