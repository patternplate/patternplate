{
  "name": "@patternplate/components",
  "version": "2.5.25",
  "description": "ui component library for patternplate-client",
  "scripts": {
    "start": "yarn build -w",
    "build": "babel -D src -d lib --source-maps",
    "watch": "yarn build -w",
    "deps": "dependency-check . lib/**/demo.js --missing && dependency-check . lib/**/demo.js  --extra --no-dev"
  },
  "files": [
    "cover.js",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patternplate/patternplate.git"
  },
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Daniel GooÃŸ",
      "email": "daniel.gooss@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "xo": false,
  "babel": {
    "presets": [
      [
        "module:@patternplate/babel-preset",
        {
          "targets": [
            "node",
            "web"
          ],
          "sources": [
            "react",
            "styled-components"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@patternplate/babel-preset": "^2.5.18",
    "@patternplate/render-styled-components": "^2.5.18",
    "concurrently": "^3.5.1",
    "dependency-check": "^3.1.0",
    "react-clipboard.js": "^1.1.3"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "codemirror": "^5.36.0",
    "color": "^2.0.0",
    "front-matter": "^2.3.0",
    "hast-to-hyperscript": "^3.0.2",
    "iframe-resizer": "^3.5.16",
    "lodash": "^4.17.4",
    "parse-numeric-range": "^0.0.2",
    "prop-types": "^15.6.0",
    "query-string": "^6.0.0",
    "react": "16.2.0",
    "react-addons-text-content": "^0.0.4",
    "react-codemirror2": "^4.2.1",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-side-effect": "^1.0.2",
    "refractor": "^2.6.0",
    "remark": "^8.0.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-react": "^4.0.3",
    "semver": "^5.5.0",
    "styled-components": "^3.1.6",
    "tag-hoc": "^1.0.0",
    "xmldom": "^0.1.27"
  }
}
