{
  "name": "@patternplate/component-svg",
  "version": "3.2.3",
  "description": "Render sanitized SVG as react components",
  "main": "lib/svg.js",
  "scripts": {
    "clean": "rimraf lib",
    "deps": "dependency-check . --missing && dependency-check . --extra --no-dev"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patternplate/patternplate.git"
  },
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "patternplate": {
    "name": "component-svg",
    "displayName": "Component: SVG",
    "flag": "stable"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "dependency-check": "^3.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.1"
  },
  "dependencies": {
    "@patternplate/component-utility": "^3.1.1",
    "@types/btoa": "^1.2.0",
    "@types/react": "16.7.7",
    "@types/styled-components": "^4.1.1",
    "lodash": "^4.17.11",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "styled-components": "^4.1.1",
    "xmldom": "^0.1.27"
  }
}
